{
  "name": "pdf-depersonalization-in-web",
  "version": "1.0.3",
  "description": "Веб-приложение для загрузки PDF, рисования поверх его страниц и сохранения результата.",
  "main": "src/script.js",
  "homepage": "https://riv-gh.github.io/pdf-depersonalization-in-web",
  "dockerhub": "daenur/pdf-depersonalization-in-web",
  "scripts": {
    "dev": "webpack serve --mode development --open",
    "build": "webpack --mode production",
    "deploy": "npm run build && gh-pages --no-history -d dist",
    "set-version": "node ./npm-run-scripts/set-version.js",
    "docker-build": "node -e \"const pkg=require('./package.json'); require('child_process').execSync(`docker build -t ${pkg.dockerhub}:latest -t ${pkg.dockerhub}:${pkg.version} .`, {stdio: 'inherit'});\"",
    "docker-push": "node -e \"const pkg=require('./package.json'); require('child_process').execSync(`docker push ${pkg.dockerhub}:latest && docker push ${pkg.dockerhub}:${pkg.version}`, {stdio: 'inherit'});\""
  },
  "dependencies": {
    "jspdf": "^2.5.1",
    "pdfjs-dist": "^3.9.320"
  },
  "devDependencies": {
    "css-loader": "^6.7.3",
    "gh-pages": "^6.3.0",
    "html-webpack-plugin": "^5.5.0",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "yaml-loader": "^0.8.1"
  },
  "browserslist": [
    "last 2 versions",
    "not dead"
  ]
}